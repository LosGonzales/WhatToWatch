<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#1D2025</item>
        <item name="colorPrimaryDark">#1D2025</item>
        <item name="colorAccent">#EDC298</item>
    </style>

    <style name="AppTheme.NoActionBar" parent="Theme.MaterialComponents.DayNight">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">#1D2025</item>
        <item name="colorPrimaryDark">#1D2025</item>
        <item name="colorAccent">#EDC298</item>
    </style>

    <style name="DialogTheme" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorAccent">#1D2025</item>
        <item name="android:textColorPrimary">@color/white</item>

    </style>


</resources>